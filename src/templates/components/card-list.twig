{#
  Card List

  Data is passed onto each card

  @param {array}  items
  {
    See card.twig
  }
#}

{# Output #}
<section class="c-card-list container">
  <div class="row">
    {% for item in params.items %}
    <div class="col-12 lg:col-4 mb-12 lg:mb-0">
      {% include '/templates/components/card.twig' with { params: item } %}
    </div>
    {% endfor %}
  </div>
</section>
